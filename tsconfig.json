{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "es2015",
      "dom",
      "webworker"
    ],
    "skipDefaultLibCheck": true,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "~*": [
        "./src*"
      ]
    },
    "jsx": "react-jsx",
    "strict": true,
    "sourceMap": true
  }
}